{
  "family": "react-vite-app-dev",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::821410798710:role/react-vite-app-dev-ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "react-vite-app-container",
      "image": "821410798710.dkr.ecr.us-west-1.amazonaws.com/react-vite-app-dev:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/react-vite-app-dev",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}